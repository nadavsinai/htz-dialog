    "init": "git init && npm i",
    "jsdoc": "config/jsdoc.sh",
    "lint": "eslint ./src/**/*.js",
    "prepublish": "npm run build",
    "serve": "browser-sync start --s dist ",
    "start": "webpack-dev-server --inline --progress --colors --contentBase=static",
    "tdd:browser": "npm run test:browser -- --auto-watch --no-single-run --browsers Chrome",
    "test:browser": "karma start",
    "test": "cross-env NODE_ENV=test nyc mocha-webpack",
    "tdd": "npm test -- --watch",
    "coverage:report": "nyc report --reporter=html"
  },
