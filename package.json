{
  "name": "htz-dialog",
  "version": "2.0.1",
  "description": "Accessible dialog and modal windows",
  "repository": {
    "type": "git",
    "url": "https://github.com/haaretz/htz-dialog"
  },
  "license": "MIT",
  "scripts": {
    "init": "git init && npm i",
    "test": "karma start",
    "tdd": "karma start --no-signle-run --auto-watch",
    "test:mocha": "BABEL_ENV=test nyc  mocha --require ./tests/mochaEnv.js src/**/*.spec.js",
    "test:mocha:tdd": "BABEL_ENV=test mocha --watch --require ./tests/mochaEnv.js src/**/*.spec.js",
    "coverage:report": "nyc report --reporter=html"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.14.0",
    "browser-sync": "^2.15.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "del": "^2.2.2",
    "dirty-chai": "^1.2.2",
    "es-module-loader": "^1.3.5",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-if": "^2.0.1",
    "gulp-ignore": "^2.0.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-newer": "^1.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sequence": "^0.4.6",
    "gulp-shell": "^0.5.2",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.0",
    "jsdom": "^9.9.1",
    "jsdom-global": "^2.1.1",
    "jspm": "^0.17.0-beta.31",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserstack-launcher": "^1.1.1",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "douglasduteil/karma-coverage#next",
    "karma-coveralls": "^1.1.2",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jspm": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sauce-launcher": "^1.1.0",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "loke-jsdoc-theme": "^2.1.0",
    "mocha": "^3.2.0",
    "nyc": "^10.0.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "systemjs-istanbul-hook": "^0.1.1",
    "tern": "^0.20.0"
  },
  "dependencies": {},
  "jspm": {
    "name": "htz-dialog",
    "main": "src/index.js",
    "directories": {
      "lib": "src"
    },
    "dependencies": {
      "htz-dispatch-event": "github:haaretz/htz-dispatch-event@^1.0.3",
      "htz-get-focusables": "github:haaretz/htz-get-focusables@^1.0.2",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.17"
    },
    "devDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "chai": "npm:chai@^3.5.0",
      "chai-as-promised": "npm:chai-as-promised@^6.0.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "dirty-chai": "npm:dirty-chai@^1.2.2",
      "es-module-loader": "npm:es-module-loader@^1.3.5",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0"
    },
    "overrides": {
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      }
    }
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
