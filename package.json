{
  "name": "htz-dialog",
  "version": "2.0.1",
  "description": "Accessible dialog and modal windows",
  "repository": {
    "type": "git",
    "url": "https://github.com/haaretz/htz-dialog"
  },
  "license": "MIT",
  "module": "src/index.js",
  "main": "dist/htzDialog.min.js",
  "scripts": {
    "update": "ncu",
    "build": "cross-env NODE_ENV=production webpack -p --progress --display-error-details --color",
